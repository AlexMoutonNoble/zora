{
  "name": "zora",
  "version": "1.1.3",
  "description": "tap test harness for nodejs and browsers",
  "keywords": [
    "tap",
    "test",
    "node",
    "browsers",
    "testing",
    "tests",
    "harness",
    "tap-producer"
  ],
  "main": "./dist/zora.js",
  "module": "./dist/zora.es.js",
  "jsnext:main": "./dist/zora.es.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "clean:test": "rm -rf ./test/dist",
    "clean:benchmark": "rm -r ./benchmarks && mkdirp ./benchmarks/zora/test/ ./benchmarks/ava/test ./benchmarks/jest/test ./benchmarks/mocha/test ./benchmarks/tape/test",
    "build:test": "rollup -c ./rollup/test.js",
    "build:es": "rollup -c ./rollup/es.js",
    "build:umd": "rollup -c ./rollup/umd.js",
    "build:benchmark": "npm run clean:benchmark && node ./scripts/generate.js",
    "bench:zora": "time (node ./benchmarks/zora/index.js | tap-diff)",
    "bench:ava": "time ava ./benchmarks/ava/test/",
    "bench:mocha": "time mocha ./benchmarks/mocha/test/",
    "bench:tape": "time (node ./benchmarks/tape/index)",
    "bench:jest": "time jest",
    "sample": "node ./test/sample.js",
    "build": "npm run clean && npm run build:es && npm run build:umd",
    "test": "xo ./lib/*.js && npm run clean:test && npm run build:test && node ./test/dist/index.js"
  },
  "author": {
    "name": "Laurent Renard",
    "email": "laurent34azerty@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "zora-reporter": "0.0.2"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "jest": "^22.1.1",
    "mkdirp": "^0.5.1",
    "mocha": "^4.1.0",
    "rollup": "^0.54.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.8.0",
    "xo": "^0.18.2"
  },
  "jest": {
    "testRegex": ".*.js",
    "roots": [
      "./benchmarks/jest/test"
    ],
    "testEnvironment": "node"
  }
}
