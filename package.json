{
  "name": "zora",
  "version": "2.0.1",
  "description": "tap test harness for nodejs and browsers",
  "keywords": [
    "tap",
    "test",
    "node",
    "browsers",
    "testing",
    "tests",
    "harness",
    "tap-producer"
  ],
  "main": "./dist/bundle/index",
  "module": "./dist/bundle/module.js",
  "types": "./dist/declarations/index.d.ts",
  "files": [
    "dist/bundle",
    "dist/declarations"
  ],
  "scripts": {
    "build:clean": "rm -rf ./dist && mkdir -p ./dist/bundle ./dist/declarations && cp -r ./src ./dist/src",
    "build:compile": "tsc",
    "build:bundle": "rollup -c ./rollup/build.js && rollup -c ./rollup/cdn.js",
    "build": "npm run build:clean && npm run build:compile && npm run build:bundle",
    "clean:benchmark": "rm -r ./benchmarks && mkdirp ./benchmarks/zora/test/ ./benchmarks/ava/test ./benchmarks/jest/test ./benchmarks/mocha/test ./benchmarks/tape/test",
    "build:benchmark": "npm run clean:benchmark && node ./scripts/generate.js",
    "bench:zora": "time (node ./benchmarks/zora/index.js)",
    "bench:ava": "time ava ./benchmarks/ava/test/",
    "bench:mocha": "time mocha ./benchmarks/mocha/test/",
    "bench:tape": "time (node ./benchmarks/tape/index)",
    "bench:jest": "time jest",
    "test:unit": "rollup -c ./rollup/test.js | node",
    "test": "npm run test:unit && node ./test/samples/index.js | tap-diff && node ./test/samples/bsd/index | tap-mocha-reporter spec"
  },
  "author": {
    "name": "Laurent Renard",
    "email": "laurent34azerty@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "deep-equal": "^1.0.1"
  },
  "devDependencies": {
    "@lorenzofox3/for-await": "^0.2.0",
    "ava": "^0.25.0",
    "fast-deep-equal": "^2.0.1",
    "faucet": "0.0.1",
    "jest": "^23.6.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "rollup": "^0.67.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "tap": "^12.1.1",
    "tap-diff": "^0.1.1",
    "tap-mocha-reporter": "^3.0.7",
    "tap-summary": "^4.0.0",
    "tape": "^4.9.1",
    "typescript": "^3.2.2",
    "xo": "^0.23.0"
  },
  "jest": {
    "testRegex": ".*.js",
    "roots": [
      "./benchmarks/jest/test"
    ],
    "testEnvironment": "node"
  }
}
