{
  "name": "zora",
  "version": "1.1.2",
  "description": "tap test harness for nodejs and browsers",
  "keywords": [
    "tap",
    "test",
    "node",
    "browsers",
    "testing",
    "tests",
    "harness",
    "tap-producer"
  ],
  "main": "./dist/zora.js",
  "module": "./dist/zora.es.js",
  "jsnext:main": "./dist/zora.es.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "clean:test": "rm -rf ./test/dist",
    "build:test": "rollup -c ./rollup/test.js",
    "build:es": "rollup -c ./rollup/es.js",
    "build:umd": "rollup -c ./rollup/umd.js",
    "sample": "node ./test/sample.js | faucet",
    "build": "npm run clean && npm run build:es && npm run build:umd",
    "test": "xo ./lib/*.js && npm run clean:test && npm run build:test && node ./test/dist/index.js"
  },
  "author": {
    "name": "Laurent Renard",
    "email": "laurent34azerty@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "zora-reporter": "0.0.2"
  },
  "devDependencies": {
    "rollup": "^0.53.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "tape": "^4.8.0",
    "xo": "^0.18.2"
  }
}
